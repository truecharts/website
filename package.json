{
  "name": "website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": 'export NODE_OPTIONS=--max-old-space-size=22000 && find . -name "CHANGELOG.md" -type f -exec rm -f {} + && astro dev',
    "dev-windows": 'set NODE_OPTIONS=--max-old-space-size=22000 && find . -name "CHANGELOG.md" -type f -exec rm -f {} +  && astro dev',
    "start": 'export NODE_OPTIONS=--max-old-space-size=22000 && find . -name "CHANGELOG.md" -type f -exec rm -f {} +  && astro dev',
    "check": 'find . -name "CHANGELOG.md" -type f -exec rm -f {} + && astro check",
    "build": 'export NODE_OPTIONS=--max-old-space-size=22000 && find . -name "CHANGELOG.md" -type f -exec rm -f {} +  && astro build',
    "preview": 'find . -name "CHANGELOG.md" -type f -exec rm -f {} + && astro preview',
    "astro": 'find . -name "CHANGELOG.md" -type f -exec rm -f {} + && astro'
  },
  "dependencies": {
    "@astrojs/check": "0.7.0",
    "@astrojs/sitemap": "3.1.4",
    "@astrojs/starlight": "0.23.1",
    "@astrojs/starlight-tailwind": "2.0.2",
    "@astrojs/tailwind": "5.1.0",
    "@playform/compress": "0.0.4",
    "astro": "4.8.6",
    "astro-better-image-service": "1.0.23",
    "astro-integration-lottie": "0.3.0",
    "astro-robots-txt": "1.0.0",
    "lottie-web": "5.12.2",
    "sharp": "0.33.4",
    "starlight-blog": "0.8.2",
    "starlight-image-zoom": "0.4.0",
    "starlight-links-validator": "0.8.0",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  }
}
